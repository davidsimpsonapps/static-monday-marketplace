---
layout: base.njk
pagination:
  data: marketplace
  size: 1
  alias: app
  addAllPagesToCollections: true
permalink: /apps/{{ app.id }}/
eleventyComputed:
  title: "{{app.name | safe}}"
  description: "{{app.name | safe}} (app for monday.com). {{app.short_description}}."
  keywords: "{{app.keywords}},app for monday.com" 
  canonical: "/apps/{{ app.id }}/#/{{ app.name | sanitisePathname }}"
  image: "{{ app.logo_url }}"
---

{% set vendor = vendors | findVendorById(app.marketplace_developer_id) %}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tocbot/4.36.4/tocbot.css" integrity="sha512-nJwmkYKmBxqHBZfKnXTiVA2YVIrRULgO+gn6m70r4vqSMCRtQKmRSfd9LozVxcOR7V9/LV0TWLqkmspIHhr1KA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

{% if vendor %}
  <div class="breadcrumb">
    <span>
      <a href="/">
        <svg viewBox="0 0 20 20" fill="currentColor" width="14" height="14" class="breadcrumbIcon_d94850cf8c"><path d="M9.56992 2.1408C9.82591 1.95307 10.1741 1.95307 10.4301 2.1408L17.7028 7.47413C17.8896 7.61113 18 7.82894 18 8.06061V16.7879C18 17.1895 17.6744 17.5152 17.2727 17.5152H11.9394C11.5377 17.5152 11.2121 17.1895 11.2121 16.7879V13.1515H8.78788V16.7879C8.78788 17.1895 8.46227 17.5152 8.06061 17.5152H2.72727C2.32561 17.5152 2 17.1895 2 16.7879V8.06061C2 7.82894 2.11037 7.61113 2.29719 7.47413L9.56992 2.1408ZM3.45455 8.42914V16.0606H7.33333V12.4242C7.33333 12.0226 7.65894 11.697 8.06061 11.697H11.9394C12.3411 11.697 12.6667 12.0226 12.6667 12.4242V16.0606H16.5455V8.42914L10 3.62914L3.45455 8.42914Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
        <span>Home</span>
      </a>
    </span>

    <span>
      <svg viewBox="0 0 20 20" fill="currentColor" width="14" height="14" class="separatorIcon_3a648a2555" aria-hidden="true"><path d="M13.5303 9.46967L13 10L13.5303 10.5303C13.8232 10.2374 13.8232 9.76256 13.5303 9.46967ZM11.9393 10L5.46967 16.4697C5.17678 16.7626 5.17678 17.2374 5.46967 17.5303C5.76256 17.8232 6.23744 17.8232 6.53033 17.5303L13.5303 10.5303L13 10L13.5303 9.46967L6.53033 2.46967C6.23744 2.17678 5.76256 2.17678 5.46967 2.46967C5.17678 2.76256 5.17678 3.23744 5.46967 3.53033L11.9393 10Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
    </span>
    <span>
      <a href="/vendors/" >
        Vendors
      </a>
    </span>    
    <span>
      <svg viewBox="0 0 20 20" fill="currentColor" width="14" height="14" class="separatorIcon_3a648a2555" aria-hidden="true"><path d="M13.5303 9.46967L13 10L13.5303 10.5303C13.8232 10.2374 13.8232 9.76256 13.5303 9.46967ZM11.9393 10L5.46967 16.4697C5.17678 16.7626 5.17678 17.2374 5.46967 17.5303C5.76256 17.8232 6.23744 17.8232 6.53033 17.5303L13.5303 10.5303L13 10L13.5303 9.46967L6.53033 2.46967C6.23744 2.17678 5.76256 2.17678 5.46967 2.46967C5.17678 2.76256 5.17678 3.23744 5.46967 3.53033L11.9393 10Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
    </span>
    <span>
      <a href="/vendors/{{ vendor.id }}/#/{{ vendor.name | sanitisePathname }}" >
        {{ vendor.name }}
      </a>
    </span>
    <span>
      <svg viewBox="0 0 20 20" fill="currentColor" width="14" height="14" class="separatorIcon_3a648a2555" aria-hidden="true"><path d="M13.5303 9.46967L13 10L13.5303 10.5303C13.8232 10.2374 13.8232 9.76256 13.5303 9.46967ZM11.9393 10L5.46967 16.4697C5.17678 16.7626 5.17678 17.2374 5.46967 17.5303C5.76256 17.8232 6.23744 17.8232 6.53033 17.5303L13.5303 10.5303L13 10L13.5303 9.46967L6.53033 2.46967C6.23744 2.17678 5.76256 2.17678 5.46967 2.46967C5.17678 2.76256 5.17678 3.23744 5.46967 3.53033L11.9393 10Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
    </span>

    <span>
      <a href="/apps/{{ app.id }}/#/{{ app.name | sanitisePathname }}" >
        {{ app.name }}
      </a>
    </span>
  </div>
{% endif %}

<div class="sm:mx-auto sm:my-0 overflow-hidden">
  <div class="flex flex-col lg:flex-row gap-16">
    <!-- Main content -->
    <div class="flex-1">
      <div class="pb-4">
        <div class="flex flex-col md:flex-row md:items-start md:justify-between sm:gap-6">
          <div class="flex flex-col sm:flex-row sm:items-start gap-6">

            {% if app.logo_url %}
              <img src="{{ app.logo_url }}" alt="{{ app.name }} logo" 
                class="w-12 h-12 rounded">
            {% else %}
              <div class="w-12 h-12 rounded-lg bg-gray-100"></div>
            {% endif %}
            <div>
              <h1 class="text-xl font-semibold mb-2 no-tocbot">
                {{ app.name }}
                {% if app.badges_data.security === true %}
                  <span data-toggle="tooltip" title="Security badge" class="ml-1"><svg width="16px" height="16px" viewBox="0 0 16 16" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><path d="M8.1268,15.3208 C7.8518,15.4264 7.5482,15.4264 7.2732,15.3208 C2.89097361,13.6386115 -0.00112082558,9.42950309 -3.2566432e-07,4.7355 L-3.2566432e-07,1.1836 C-3.2566432e-07,0.869499083 0.124851208,0.56827897 0.347057232,0.346279265 C0.569263257,0.12427956 0.870599218,-0.000291915351 1.1847,-5.11890647e-07 L14.2153,-5.11890647e-07 C14.8691624,-5.11890647e-07 15.3993929,0.529737875 15.4000031,1.1836 L15.4000031,4.7366 C15.4034867,9.43107638 12.5104796,13.6410756 8.1268,15.3208 L8.1268,15.3208 Z" id="Path" fill="#0F9B63" fill-rule="nonzero"></path><polyline stroke="#FFFFFF" stroke-width="1.925" points="4.1382 6.1237 7.0257 9.0112 11.8382 3.7169"></polyline></g></svg></span>
                {% endif %}
                {% if app.acquisition_source === "Touch" %}
                  <span data-toggle="tooltip" title="Acquisition source: {{ app.acquisition_source }}" class="text-lg p-1">üñêÔ∏è</span> 
                {% endif %}     
              </h1>
              <p class="text-gray-600">
                {% if vendor %}
                  <a href="/vendors/{{ vendor.id }}/#/{{ vendor.name | lower | replace(' ', '-') }}" class="text-blue-600 hover:text-blue-800">
                    {{ vendor.name }}
                  </a>

                  {% if vendor.partners_program === 1 %}
                    <span data-toggle="tooltip" title="Bronze Marketplace Partner"><svg width="20" height="20" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" class="icon_a812034417 Rv7UF clickable_e41bd603c6" data-testid="icon"><circle cx="10" cy="10" r="8" fill="#C25531"></circle><path d="M10.173 15.066a7.24 7.24 0 0 1 4.88-4.657L16 10.14l-.933-.314a7.24 7.24 0 0 1-4.657-4.88L10.141 4l-.313.932a7.24 7.24 0 0 1-4.881 4.657L4 9.858l.933.313a7.24 7.24 0 0 1 4.656 4.881l.27.948.314-.934Z" fill="#fff"></path></svg></span>
                  {% elseif vendor.partners_program === 2 %}
                    <span data-toggle="tooltip" title="Silver Marketplace Partner"><svg width="20" height="20" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" class="icon_a812034417 Rv7UF clickable_e41bd603c6" data-testid="icon"><circle cx="10" cy="10" r="8" fill="gray"></circle><path d="M10.173 15.066a7.24 7.24 0 0 1 4.88-4.657L16 10.14l-.933-.314a7.24 7.24 0 0 1-4.657-4.88L10.141 4l-.313.932a7.24 7.24 0 0 1-4.881 4.657L4 9.858l.933.313a7.24 7.24 0 0 1 4.656 4.881l.27.948.314-.934Z" fill="#fff"></path></svg></span>
                  {% elseif vendor.partners_program === 3 %}
                    <span data-toggle="tooltip" title="Gold Marketplace Partner"><svg width="20" height="20" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" class="icon_a812034417 Rv7UF clickable_e41bd603c6" data-testid="icon"><circle cx="10" cy="10" r="8" fill="#FFCB00"></circle><path d="M10.173 15.066a7.24 7.24 0 0 1 4.88-4.657L16 10.14l-.933-.314a7.24 7.24 0 0 1-4.657-4.88L10.141 4l-.313.932a7.24 7.24 0 0 1-4.881 4.657L4 9.858l.933.313a7.24 7.24 0 0 1 4.656 4.881l.27.948.314-.934Z" fill="#fff"></path></svg></span>
                  {% elseif vendor.partners_program === 4 %}
                    <span data-toggle="tooltip" title="Platinum Marketplace Partner"><svg width="20" height="20" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" class="icon_a812034417 Rv7UF clickable_e41bd603c6" data-testid="icon"><circle cx="10" cy="10" r="8" fill="#5559DF"></circle><path d="M10.173 15.066a7.24 7.24 0 0 1 4.88-4.657L16 10.14l-.933-.314a7.24 7.24 0 0 1-4.657-4.88L10.141 4l-.313.932a7.24 7.24 0 0 1-4.881 4.657L4 9.858l.933.313a7.24 7.24 0 0 1 4.656 4.881l.27.948.314-.934Z" fill="#fff"></path></svg></span>
                  {% endif %}


                  {% if vendor and vendor.website %}
                    <a title="Vendor's website" href="{{ vendor.website }}" target="_blank" rel="noopener noreferrer" class="text-gray-600 hover:text-gray-800 mx-1">
                      <svg class="w-4 h-4 mr-2 inline" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"/>
                      </svg>
                    </a>
                  {% endif %}
                {% else %}
                  Unknown Vendor
                {% endif %}
              </p>
              <!--<p class="text-gray-600 mb-4">App IDs: {{ app.id }} ‚Ä¢ {{app.app_id}}</p>-->

              {% set rating = ratings | findRatingById(app.app_id) %}
              {% if rating %}
                <div class="mt-1 text-gray-400 text-sm">
                  <a href="#reviews" data-toggle="tooltip" title="See reviews">
                    <span class="text-yellow-500 ">‚òÖ</span>
                    <span class="ml-1">{{ rating.rating | formatRating }}</span>
                    <span class=" ml-1">({{ rating.count }})</span>
                  </a>
                </div>
              {% endif %}
              {% if installs %}
                <p class="text-gray-600 text-sm mb-2 sm:mb-4">
         
                  
                  {% set appInstalls =  installs | findInstallsById(app.app_id) %}
                  <a href="#installs" data-toggle="tooltip" title="See installation history"
                    >{{ appInstalls | numberFormat }} installs</a>,
                  since {{ app.created_at | formatDate }}.
                  &nbsp;
                  <span class="whitespace-nowrap">{{ appInstalls | averageInstallsPerMonth(app.created_at)}} installs/month.</span>
                  &nbsp;
                  <span class="whitespace-nowrap">Updated {{ app.app_live_version.updated_at | formatDate }}.</span>
                </p>
              {% endif %}

              <div class="flex flex-wrap gap-2 mb-2">
                {% if app | isHostedOnMonday %}
                  <span title="App & logs hosted by monday.com" class="badge green">
                    Hosted by monday.com
                  </span>
                {% endif %}

                
                {% if app.plans %}
                  <span class="badge blue">
                    Paid
                  </span>
                {% else %}
                  <span class="badge green" data-toggle="tooltip" title="No plan found. Could be free or paid elsewhere">
                    Not paid
                  </span>
                {% endif %}
                
                {% if app.label %}
                  <span  class="badge purple">
                    {{ app.label }}
                  </span>
                {% endif %}
                {% if app.acquisition_source %}
                  <span class="badge opacity-30">
                    {{ app.acquisition_source }}
                  </span>
                {% endif %}
              </div>

              {% if app.marketplace_category_ids and app.marketplace_category_ids.length > 0 %}
                <div class="flex flex-wrap gap-2 mb-4">
                  {% for categoryId in app.marketplace_category_ids %}
                    {% set category = categories | findCategoryById(categoryId) %}
                    {% if category %}
                      <a href="/categories/{{ category.id }}/" class="badge hover:bg-gray-200">
                        {{ category.name }}
                      </a>
                    {% endif %}
                  {% endfor %}
                </div>
              {% endif %}
            </div>
          </div>
          <div class="flex-shrink-0 lg:self-start">
            <div class="mb-2 sm:text-right">
              <a 
                href="https://auth.monday.com/marketplace_app_from_anywhere?app_id={{ app.id }}" 
                target="_blank" 
                rel="noopener noreferrer" 
                class="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-white   bg-blue-600 hover:bg-blue-800 hover:text-white focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                Sign in to install
              </a>
            </div>

            <div class="mb-2 sm:text-right">
              <a 
                href="https://auth.monday.com/marketplace/listing/{{ app.id }}" 
                target="_blank" 
                rel="noopener noreferrer" 
                class="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500"
                >
                <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"/>
                </svg>
                View on Marketplace
              </a>
            </div>
          </div>
        </div>
      </div>

      {% if app.gallery_assets and app.gallery_assets.length > 0 %}
        
          <div class="">
            <div class="grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-4">
              
              {% for asset in app.gallery_assets %}
                <a data-fslightbox href="{{ asset.url}}">
                  {% if asset.type == 'video' %}
                    <video controls class="rounded-lg shadow-md">
                      <source src="{{ asset.url}}" />
                    </video>
                  {% else %}
                    <img src="{{ asset.url }}" alt="Gallery image" class="rounded shadow-md">
                  {% endif %}
                </a>
              {% endfor %}
            </div>
          </div>
        
      {% endif %}



      
      <div class="py-8">
        <h2 class="text-lg sm:text-2xl font-semibold mb-4">{{ app.short_description }}</h2>
        <div class="prose max-w-none">
          {{ app.description | safe }}
        </div>
      </div>
      
      {% if app.support_contact %}
        
          <div class="py-8">
            <h2 class="text-lg sm:text-2xl font-semibold mb-4">Support</h2>
            <p class="text-gray-600">
              Contact: <a href="mailto:{{ app.support_contact }}" class="text-blue-600 hover:text-blue-800">{{ app.support_contact }}</a>
            </p>
          </div>
        
      {% endif %}



      {% if complianceQuestions and complianceQuestions.all.length > 0 %}
          <div class="py-8">
            <h2 class="text-lg sm:text-2xl font-semibold mb-6">Security & Compliance</h2>

            {% if app.compliance_answers and app.compliance_answers | length > 0 %}

              {% for type in ['Security', 'Compliance', 'Data', 'Privacy'] %}
                {% set typeQuestions = [] %}
                {% for question in complianceQuestions.all %}
                  {% if question.type == type %}
                    {% set typeQuestions = typeQuestions | concat([question]) %}
                  {% endif %}
                {% endfor %}
                
                {% if typeQuestions.length > 0 %}
                  <div class="mb-8">
                    <h3 class="text-xl font-medium mb-4">{{ type }}</h3>
                    <div class="space-y-4">
                      {% for question in typeQuestions %}
                        <div class="bg-gray-50 rounded-lg p-4">
                          <div class="flex flex-col w-full">
                            <div class="flex justify-between items-start">
                              <div class="flex-1">
                                <p class="font-medium mb-2">{{ question.question }}</p>
                              </div>
                              {% if app.compliance_answers %}
                                {% set answer = null %}
                                {% for ans in app.compliance_answers %}
                                  {% if ans.questionId === question.id %}
                                    {% set answer = ans %}
                                  {% endif %}
                                {% endfor %}
                                {% if answer %}
                                  <div class="ml-4 mt-2 flex items-center">
                                    {% if answer.shortAnswer === true %}
                                        <span class="text-green-600 font-medium">Yes</span>
                                    {% elseif answer.shortAnswer === false %}
                                        <span class="text-red-600 font-medium">No</span>
                                    {% else %}
                                        <span class="text-gray-600">{{ answer.answer }}</span>
                                    {% endif %}

                                    {% if answer and answer.logHostingProvider %}
                                      <div class="text-green-600">
                                        {{ answer.logHostingProvider }}
                                      </div>
                                    {% endif %}
                                    {% if answer and answer.dataHostingProvider %}
                                      <div class="text-green-600">
                                        {{ answer.dataHostingProvider }}
                                      </div>
                                    {% endif %}
                                  </div>
                                {% else %}
                                  <div class="ml-4 mt-2 text-gray-400">Not answered</div>
                                {% endif %}
                              {% else %}
                                <div class="ml-4 mt-2 text-gray-400">Not answered</div>
                              {% endif %}
                            </div>
                            {% if answer and answer.detailedAnswer %}
                              <div class="mt-2 text-gray-600">
                                {{ answer.detailedAnswer }}
                              </div>
                            {% endif %}
                          </div>
                        </div>
                      {% endfor %}
                    </div>
                  </div>
                {% endif %}
              {% endfor %}

            {% else %}
              <p>The app developer did not provide the security and compliance information for this app.</p>
            {% endif %}
          </div>
      {% endif %}


      <!-- Reviews Section -->
      <div class="py-8">
        <h2 id="reviews" class="text-lg sm:text-2xl leading-6 font-medium text-gray-900">Reviews</h2>
      
        {% set appReviews = reviews[app.app_id] %}
        {% if appReviews and appReviews.length > 0 %}
          
            {% for review in appReviews %}
              <div class="py-4 border-t border-gray-200">
                <div class="flex items-center mb-2">
                  <div class="flex items-center">
                    {% for i in range(1, 6) %}
                      {% if i <= review.rating %}
                        <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
                          <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
                        </svg>
                      {% else %}
                        <svg class="w-5 h-5 text-gray-300" fill="currentColor" viewBox="0 0 20 20">
                          <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
                        </svg>
                      {% endif %}
                    {% endfor %}
                  </div>
                  <span class="ml-2 text-sm text-gray-500">
                    {{ review.commentedAt | formatDate }}
                  </span>
                </div>
                <p class="text-gray-700">
                  <b>{{review.reviewerNameInitials}}:</b>  
                  {{ review.comment }}
                </p>
                {% if review.developerReply %}
                  <div class="mt-2 pl-4 border-l-2 border-gray-200">
                    <p class="text-sm text-gray-500">Developer response:</p>
                    <p class="text-gray-700">{{ review.developerReply }}</p>
                  </div>
                {% endif %}
              </div>
            {% endfor %}
          
        {% else %}
          <div class="py-4">
            <p class="text-gray-500">No reviews yet.</p>
          </div>
        {% endif %}
      </div>


      <!-- Install history since 2025-06-07 -->
      <div class="py-8">
        <h2 id="historical" class="text-lg sm:text-2xl font-semibold mb-4">Historical data</h2>
        
        <h3 id="installs" class="text-xl font-medium mb-4">Installation history</h3>

        <p class="text-xs text-gray-600">We have data for December 28, 2024 onwards only. Collected sometime after 00:00 UTC daily.</p>
        
        <div class="chart-container">
          <h4 id="installsAll" class="font-semibold">Total number of installs</h4>
          <div class="h-96">
            <canvas id="installChart"></canvas>
          </div>
        </div>
        
        <div class="chart-container">
          <h4 id="installs-7d" class="font-semibold">Change in total number of installs in last 7 days</h4>
          <p class="text-xs text-gray-600">Compares the number of installs on each date with 7 days previously:</p>        
          <div id="headlines-7d" class="headlines-container" >
            {% include "partials/headlines.njk" %}
          </div>
          <div class="h-96">
            <canvas id="installsChart-7d"></canvas>
          </div>
        </div> 

        <div class="chart-container">
          <h4 id="installs-30d" class="font-semibold">Change in total number of installs in last 30 days</h4>
          <p class="text-xs text-gray-600">Compares the number of installs on each date with 30 days previously:</p>
          <div id="headlines-30d" class="headlines-container" >
            {% include "partials/headlines.njk" %}
          </div>          
          <div class="h-96">
            <canvas id="installsChart-30d"></canvas>
          </div>
        </div>  

        <div class="chart-container">
          <h4 id="installs-90d" class="font-semibold">Change in total number of installs in last 90 days</h4>
