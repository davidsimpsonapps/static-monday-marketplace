<script>
  // Function to generate a valid ID from text
  function generateId(text) {
    return text
      .toLowerCase()
      .replace(/[^a-z0-9]+/g, '-') // Replace non-alphanumeric chars with hyphens
      .replace(/^-+|-+$/g, '') // Remove leading/trailing hyphens
      .replace(/-+/g, '-'); // Replace multiple hyphens with single hyphen
  }

  // Add IDs to headings that don't have them
  document.querySelectorAll('h1:not(.no-tocbot), h2:not(.no-tocbot), h3:not(.no-tocbot)').forEach(heading => {
    if (!heading.id) {
      heading.id = generateId(heading.textContent);
    }
  });

  tocbot.init({
    tocSelector: '.js-toc',
    contentSelector: '.flex-1',
    headingSelector: 'h1:not(.no-tocbot), h2:not(.no-tocbot), h3:not(.no-tocbot)',
    hasInnerContainers: true,
    scrollSmooth: true,
    scrollSmoothDuration: 420,
    scrollSmoothOffset: -120,
    headingsOffset: 40,
    collapseDepth: 0,
    orderedList: false
  });
</script>

<script>
function toggleDebug() {
